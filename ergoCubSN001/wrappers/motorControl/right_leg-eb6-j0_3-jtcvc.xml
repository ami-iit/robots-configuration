<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE devices PUBLIC "-//YARP//DTD yarprobotinterface 3.0//EN" "http://www.yarp.it/DTD/yarprobotinterfaceV3.0.dtd">
<device xmlns:xi="http://www.w3.org/2001/XInclude" name="right_leg-eb6-j0_3-jtcvc" type="JointTorqueControlDevice">
    <param name="remote">  /hijackingTrqCrl/r_upper_leg  </param>
    <param name="joint_list"> (  "r_hip_pitch", "r_hip_roll", "r_hip_yaw", "r_knee"  )</param>

    <group name="TORQUE_CURRENT_PARAMS">
        <param name="kt">(   0.111,  0.047,  0.047,  0.111   )</param>
        <param name="kfc">(  0.7, 0.8, 0.8, 0.8  )</param>
        <param name="kp">(  5.0, 1.0, 2.0, 1.5  )</param>
        <param name="ki">(  0.0, 0.0, 0.0, 0.0  )</param>
        <param name="max_curr">(  10.0, 10.0, 10.0, 6.0   )</param>
        <param name="friction_model"> ("FRICTION_PINN", "FRICTION_PINN", "FRICTION_PINN", "FRICTION_PINN") </param>
        <param name="bwfilter_cutoff_freq"> 2.0 </param>
        <param name="bwfilter_order"> 1 </param>
        <param name="bwfilter_enabled"> true </param>
        <param name="max_output_friction">(  25.0, 20.0, 10.0, 20.0  )</param>
    </group>

    <group name="FRICTION_PINN">
        <param name="model"> ("2_rhp_b4551_h18.onnx", "2_rhr_b4813_h20.onnx", "2_rhy_b4789_h25.onnx", "2_rk_b4184_h20.onnx") </param>
        <param name="thread_number"> 1 </param>
        <param name="model_type"> 2 </param>
    </group>

    <group name="FRICTION_COULOMB_VISCOUS">
        <param name="kv">(  0.0058    0.2367  0.0058    5.5491  )</param>
        <param name="kc">(  0.1       1.2066  0.1       8.1122  )</param>
        <param name="ka">(  53.107    10.5296  53.107    50.8400 )</param>
    </group>

    <group name="FRICTION_COULOMB_VISCOUS_STRIBECK">
        <param name="kv">(   0.1      0.2874  0.1    2.4791   )</param>
        <param name="kc">(   14.8     1.0005  14.82  5.4554   )</param>
        <param name="ks">(   13.0   6.0174  13.016   13.3471   )</param>
        <param name="ka">(   53.1    2.7803   53.10    107.9275   )</param>
        <param name="vs">(   -0.21    0.1288  -0.21   0.1486   )</param>
        <param name="alpha">(  0.0    0.6351  0.0    -0.6191   )</param>
    </group>

    <!-- Warning, still in milliseconds -->
    <param name="period"> 1 </param>

    <action phase="startup" level="4" type="attach">
        <paramlist name="networks">
            <elem name="right_leg_joints1">  right_leg-eb6-j0_3-mc </elem>
        </paramlist>
    </action>

    <action phase="shutdown" level="20" type="detach" />
</device>
