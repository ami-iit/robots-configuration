<application>
<name>iRonCubApp-Robot</name>

<var name="generic_node_1">icub29</var>

<dependencies>
</dependencies>

    <!-- _______________________iRonCub-Mk1_______________________ -->
    <module>
                <name>yarprobotinterface</name>
                <node>icub-head</node>
                <parameters>--config iRonCub_wbd.xml</parameters>
                <environment>YARP_ROBOT_NAME=iRonCub-Mk1</environment>
                <!-- <parameters>--config iRonCub-noLegs_wbd.xml</parameters> -->
    </module>

    <module>
		<name>yarplogger</name>
		<node>${generic_node_1}</node>
		<parameters>--start --no_stop</parameters>
    </module>

    <module>
                <name>yarpmotorgui</name>
                <node>${generic_node_1}</node>
                <environment>YARP_ROBOT_NAME=iRonCub-Mk1</environment>
                <parameters>--from /home/icub/code/ironcub-mk1-software/models/home-poses/homePoseiRonCub.ini</parameters>
    </module>

    
</application>
